#ifndef ABSENCODER_H_
#define ABSENCODER_H_

class absEncoder {
	GPIO_TypeDef* ports[10];
	uint8_t pins[10];
public:
	absEncoder(GPIO_TypeDef* gpioPorts[10], uint8_t gpioPins[10]);
	virtual ~absEncoder();
};

#endif /* ABSENCODER_H_ */
